@using ChatProject.Models.Entities
@model RoomsListModel

<h3>List Of your rooms</h3>

@foreach (Room room in Model.Rooms)
{
    <div class="card border-primary m-1 p-1">
        <div class="card-text p-1">@room.Name</div>
    </div>
}

<div page-model="@Model.PagingInfo" page-action="List" page-classes-enable="true"
     page-class="btn" page-class-normal="btn-secondary" 
     page-class-selected="btn-primary"
     class="btn-group float-right m-1">
    
</div>